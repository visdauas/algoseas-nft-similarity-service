    "@types/node": "16.11.64",
    "@types/supertest": "2.0.12",
    "jest": "29.1.2",
    "prettier": "2.7.1",
    "supertest": "6.3.0",
    "ts-jest": "29.0.3",
    "typescript": "4.8.4"
